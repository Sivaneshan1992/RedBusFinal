<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="05dp"
    android:layout_marginTop="05dp"
    android:background="@color/colorWhite">


    <TextView
        android:id="@+id/txt_reacheslocationin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:textColor="@color/colorBlack"
        android:textStyle="bold"
        tools:ignore="RelativeOverlap"
        tools:text="5 mins" />

    <RelativeLayout
        android:id="@+id/lay_1"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/icon_point"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="05dp"
            android:background="@drawable/point_to_point"
            android:contentDescription="@string/point_to_point_icon" />

        <TextView
            android:id="@+id/txt_starttime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="05dp"
            android:layout_marginStart="05dp"
            android:layout_toEndOf="@+id/icon_point"
            android:layout_toRightOf="@+id/icon_point"
            android:textColor="@color/colorDarkGrey"
            android:textSize="14sp"
            tools:text="9.28 pm" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txt_starttime"
            android:layout_marginLeft="05dp"
            android:layout_marginStart="05dp"
            android:layout_toEndOf="@+id/icon_point"
            android:layout_toRightOf="@+id/icon_point"
            android:orientation="vertical">


            <TextView
                android:id="@+id/txt_travelsname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="05dp"
                android:textColor="@color/colorBlack"
                android:textSize="14sp"
                tools:text="Orange Travels Pvt Limited" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="03dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_vehicleno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorLightGrey"
                    android:textSize="12sp"
                    tools:text="Tn 57 At 8203" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="05dp"
                    android:layout_marginRight="05dp"
                    android:background="@color/colorLightGrey" />

                <TextView
                    android:id="@+id/txt_vehicletype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorLightGrey"
                    android:textSize="12sp"
                    tools:text="Ac Volvo Seater" />
            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/txt_endtime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="05dp"
            android:layout_marginStart="05dp"
            android:layout_toEndOf="@+id/icon_point"
            android:layout_toRightOf="@+id/icon_point"
            android:textColor="@color/colorLightGrey"
            android:textSize="14sp"
            tools:text="12.20 Am" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lay_1"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp">


        <TextView
            android:id="@+id/txt_avg_ratings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/roundcorner_layout_green"
            android:paddingBottom="02dp"
            android:paddingLeft="06dp"
            android:paddingRight="06dp"
            android:paddingTop="02dp"
            android:textColor="@color/colorWhite"
            android:textSize="14sp"
            tools:text="4.5" />


        <TextView
            android:id="@+id/txt_totalratings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:textColor="@color/colorLightGrey"
            android:textSize="10sp"
            tools:ignore="SmallSp"
            tools:text="120 Ratings" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/lay_1"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="15dp">

        <TextView
            android:id="@+id/txt_seatsleft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/colorDarkGrey"
            android:textSize="12sp"
            tools:text="16 Seats Left" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="05dp"
            android:layout_marginRight="05dp"
            android:background="@color/colorDarkGrey" />

        <TextView
            android:id="@+id/txt_fare"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="05dp"
            android:layout_marginStart="05dp"
            android:textColor="@color/colorBlack"
            android:textSize="12sp"
            android:textStyle="bold"
            tools:text="$ 1249" />

    </LinearLayout>

</RelativeLayout>

